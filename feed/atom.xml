---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Antoine Toulme's blog</title>
 <link href="http://www.worserer.com/feed" rel="self"/>
 <link href="http://www.worserer.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://worserer.com/</id>
 <author>
   <name>Antoine Toulme's blog</name>
   <email>antoine@lunar-ocean.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.worserer.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://worserer.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>